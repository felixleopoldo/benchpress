            },
            "score": {
                "type": "string"
            },
            "iss": {
                "$ref": "#/$defs/flexnonnegnumnull"
            },
            "issmu": {
                "$ref": "#/$defs/flexnonnegnumnull"
            },
            "issw": {
                "$ref": "#/$defs/flexnonnegnumnull"
            },
            "l": {
                "$ref": "#/$defs/flexnonnegintnull"
            },
            "k": {
                "$ref": "#/$defs/flexnonnegintnull"
            },
            "prior": {
                "type": "string"
            },
            "timeout": {
                "$ref": "#/$defs/flexnatnumnull"
