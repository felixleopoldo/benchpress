rule:
    name:
        module_name
    input:
        data=alg_input_data(),
        edgeConstraints_formatted="resources/constraints/{edgeConstraints}-bnlearn"
    output:
        adjmat=alg_output_adjmat_path(module_name),
        time=alg_output_time_path(module_name),
        ntests=alg_output_ntests_path()
    container:
        "docker://bpimages/bnlearn:4.8.3"
    script:
        "script.R"
