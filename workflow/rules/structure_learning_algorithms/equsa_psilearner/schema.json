{
    "description": "Psi-learner R package",
    "title": "prsi_learner",
    "type": "array",
    "items": {
        "title": "",
        "description": "",
        "type": "object",
        "properties": {
            "id": {
                "type": "string",
                "description": "Unique identifier"
            },
            "timeout": {
                "$ref": "#/$defs/flexnonnegnumnull"
            },
            "alpha1": {
                "description": "The significance level of correlation screening. In general, a high significance level of correlation screening will lead to a slightly large separator.",
                "$ref": "#/$defs/flexprob",
                "default": 0.05
            },
            "alpha2": {
                "description": "The significance level of \u03c8 screening.",
                "$ref": "#/$defs/flexprob",
                "default": 0.05
            },
            "grid": {
                "description": "The number of components for the \u03c8 scores. The default value is 2.",
                "$ref": "#/$defs/flexnonnegint",
                "default": 2
            },
            "iterations": {
                "description": "Number of iterations for screening. The default value is 100.",
                "default": 100
            },
            "neig": {
                "description": "Neiborhood size in correlation screening step, default to n/log(n), where n is the number of observation.",
                "$ref": "#/$defs/flexnonnegint"
            }
        },
        "additionalProperties": true,
        "required": [
            "id",
            "timeout"
        ],
        "examples": [
            {
                "id": "psilearn",
                "timeout": null,
                "alpha1": 0.05,
                "alpha2": 0.05,
                "grid": 2,
                "iterations": 100,
                "neig": 10
            }
        ]
    }
}